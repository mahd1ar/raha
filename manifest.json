{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "permissions": [
    "<all_urls>",
    "storage",
    "unlimitedStorage",
    "topSites",
    "contextMenus",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index123.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}